//Grade Evaluation
#include <stdio.h>

int main()
{
     int mark;
    printf("Enter your score :");
    scanf("%d",&mark);
    
    if(mark>=90)
    {
         printf("Grade A");
         
    }
    else if(mark<=89&&mark>=80)
    {
         printf("Grade B");
    }
    else if(mark<=79&&mark>=70)
    {
         printf("Grade C");
    }
    else if(mark<=69&&mark>=60)
    {
          printf("Grade D");
    }
    
    else
    {
         printf("Grade F");
    }
    
    return 0;
}
