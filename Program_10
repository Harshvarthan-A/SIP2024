// Program for restaurant menu
#include <stdio.h>

int main()
{
     int a,b;
     
    printf("Enter the item number :");
    scanf("%d",&a);

switch(a)
    {
        case(1):
        b=(a==1);
         printf("pizza");
         break;
         
        case (2):
        b=(a==2);
         printf("Burger");
         break;
         
        case (3):
        b=(a==3);
         printf("Pasta");
         break;
    
        default:
        b=(a>=4);
         printf("invalid item");
    }

    return 0;
}
